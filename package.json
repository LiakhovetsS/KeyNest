  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiakhovetsS/KeyNest.git"
  },
  "author": "Liakhovets Stas lyakhovetss@gmail.com",
  "license": "MIT",
  "keywords": [
    "key-value",
    "kv-store",
    "ttl",
    "cache",
    "in-memory",
    "storage",
    "session",
    "temporary",
    "data-store",
    "cleanup",
    "memory",
    "expirable",
    "event-emitter",
    "typescript",
    "nodejs",
    "rate-limit",
    "throttle",
    "smart-cache"
  ],
  "scripts": {
    "build:prod": "tsc && npm version patch",
